{
  "name": "api-code-generator",
  "version": "1.0.0",
  "description": "Full-stack tool to generate production-ready integration code from API documentation",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "keywords": ["api", "code-generator", "swagger", "postman", "integration", "ai", "openai"],
  "author": "API Code Generator",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "jszip": "^3.10.1",
    "swagger-parser": "^10.0.3",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.0",
    "uuid": "^9.0.1",
    "path": "^0.12.7",
    "fs-extra": "^11.1.1",
    "openai": "^4.20.1",
    "natural": "^6.10.4",
    "compromise": "^14.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
} 